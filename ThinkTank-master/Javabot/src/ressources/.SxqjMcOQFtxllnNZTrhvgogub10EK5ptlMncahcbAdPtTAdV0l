while True:
	tank.moveForward(lineno())
	tank.moveForward(lineno())
	tank.moveForward(lineno())
	tank.moveForward(lineno())
	tank.moveForward(lineno())
	tank.moveForward(lineno())
	tank.moveForward(lineno())
	tank.moveBackward(lineno())
	tank.moveBackward(lineno())
	tank.moveBackward(lineno())
	tank.moveBackward(lineno())
	tank.moveBackward(lineno())
	tank.moveBackward(lineno())